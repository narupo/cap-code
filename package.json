{
	"name": "cap-code",
	"displayName": "cap-code",
	"publisher": "narupo",
	"description": "VS Code's extension for Cap",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cap-text-command",
		"onCommand:extension.cap-input-box-command"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.cap-text-command",
				"title": "Cap Text Command"
			},
			{
				"command": "extension.cap-input-box-command",
				"title": "Cap Input Box Command"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
